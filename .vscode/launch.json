{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Console App Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net8.0/cook-consumer.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "internalConsoleOptions": "neverOpen",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Debug"
            }
        }
    ]
}
